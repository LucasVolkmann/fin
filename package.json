{
  "name": "fin",
  "version": "1.0.0",
  "description": "A personal learning project",
  "main": "./src/index.ts",
  "scripts": {
    "db:up": "cd docker && docker-compose up -d",
    "db:down": "cd docker && docker-compose down",
    "db:migration:up": "npx typeorm-ts-node-commonjs migration:run -d src/server/config/data-source.ts",
    "db:migration:down": "npx typeorm-ts-node-commonjs migration:revert -d src/server/config/data-source.ts",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "dev": "ts-node-dev ./src",
    "build": "tsc"
  },
  "author": "Lucas Eduardo Volkmann",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "better-sqlite3": "^9.4.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
